#!/bin/sh
set -e
export PS4='################################
################################ '
trap "echo TEST FAILED" EXIT
set -x
scons -c
scons -c pytest
scons -j 4 test
scons -j 4
./test-filter.sh
./test-ocr.sh
scons -j 4 debug=1 cderiv deriv
scons -c
scons -j 4 pytest
scons -c
scons -c pytest
set +x
trap "echo ALL TESTS PASS" EXIT
